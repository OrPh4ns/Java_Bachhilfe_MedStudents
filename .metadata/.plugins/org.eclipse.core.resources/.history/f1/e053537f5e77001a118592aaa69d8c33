import javafx.fxml.FXML;
import javafx.scene.control.TextField;

public class control {
	
	@FXML TextField n1;
	@FXML private TextField n2;
	//@FXML static TextArea tx;
	@FXML Label lbl;

	

	
	@FXML void Oncheck() {

		String a = n1.getText();
	//	String t = tx.getText();
		//n2.setText(t);
	//	tx.setText(a);
		lbl.setText(a);

		
	}

}
