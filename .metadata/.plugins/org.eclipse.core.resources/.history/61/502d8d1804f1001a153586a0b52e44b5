
public class Diego1 {

	public static void bool() {
		boolean a = true;
		boolean b = false;
		
		System.out.println(a&&a);
		System.out.println(a||b);
		System.out.println(a&&b);
		
	}
	
	public static void ifelse() {
		boolean a = true;
		boolean b = false;
		
		if(a==true) {
			System.out.println("Hello World");
		}
	}
	
	public static void main(String[] args) {
		//bool();
		ifelse();
		
	}
}
